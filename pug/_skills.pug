                div.progress-bar
                  .skills.mysql &nbsp;&nbsp;&nbsp;75%
                .progress-sub WordPress
                div.progress-bar
                  .skills.wp &nbsp;&nbsp;&nbsp;40%
                .progress-sub Photoshop
                div.progress-bar
                  .skills.ps &nbsp;&nbsp;&nbsp;45%
            .skills-right
                .progress-sub Illustrator
                div.progress-bar
                  .skills.illus &nbsp;&nbsp;&nbsp;25%
                .progress-sub Word
                div.progress-bar
                  .skills.word &nbsp;&nbsp;&nbsp;80%
                .progress-sub Excel
                div.progress-bar
                  .skills.excel &nbsp;&nbsp;&nbsp;60%
                .progress-sub jQuery
                div.progress-bar
                  .skills.jq &nbsp;&nbsp;&nbsp;45%
                .progress-sub GIT
                div.progress-bar
                  .skills.git &nbsp;&nbsp;&nbsp;30%
                .progress-sub Bootstrap
                div.progress-bar
                  .skills.bs &nbsp;&nbsp;&nbsp;50%
                .progress-sub Gulp
                div.progress-bar
                  .skills.gulp &nbsp;&nbsp;&nbsp;80%
                
